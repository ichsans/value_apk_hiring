<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ichsan.value">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launche"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launche_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OnBoardActivity"></activity>
        <activity android:name=".ProfileScreen" />
        <activity android:name=".FormInputScreen" />
        <activity android:name=".RegisterScreenActivity" />
        <activity android:name=".LoginScreenActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>